@model IEnumerable<Bounds.Models.b_Enterprise>

@{
    ViewBag.Title = "Index";
}

<p>
    @Html.ActionLink("新建企业", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.b_Enterprise_Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.b_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.b_Leader_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.b_Contact)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.b_Enterprise_Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.b_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.b_Leader_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.b_Contact)
        </td>
        <td>
            @Html.ActionLink("编辑", "Edit", new { id=item.ID }) |
            @Html.ActionLink("删除", "DeleteConfirmed", new { id = item.ID }, new { onclick = "return confirm('确定删除此项？')" })
        </td>
    </tr>
}

</table>

@model IEnumerable<Bounds.Models.b_Point_Details_ShowInfo>

@{
    ViewBag.Title = "My_Detail";
}
<h5>个人明细查询</h5>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.b_Point_Type_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.b_Point_Value)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.b_Event_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Create_Time)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.b_Point_Type_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.b_Point_Value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.b_Event_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Create_Time)
        </td>
    </tr>
}

</table>

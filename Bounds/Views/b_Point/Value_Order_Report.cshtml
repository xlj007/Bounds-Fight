@model IEnumerable<Bounds.Models.Value_Order_Report>
@{
    ViewBag.Title = "Value_Order_Report";
}

<table class="table table-striped table-hover">
    <tr>
        <th>排名</th>
        <th>姓名</th>
        <th>工号</th>
        <th>月份</th>
        <th>创富产值</th>
        <th>实产值</th>
        <th>虚产值</th>
        <th>总产值</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.b_RealName</td>
            <td>@item.b_WorkNum</td>
            <td>@item.TheMonth</td>
            <td>@item.b_C_Value</td>
            <td>@item.b_S_Value</td>
            <td>@item.b_X_Value</td>
            <td>@item.b_Total_Value</td>
        </tr>
    }
</table>

